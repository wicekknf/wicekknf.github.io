<!DOCTYPE html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta
			name="viewport"
			content="width=device-width, initial-scale=1.0"
		/>
		<meta
			name="description"
			content="Salon pielęgnacji urody"
		/>

		<link
			rel="preconnect"
			href="https://fonts.googleapis.com"
		/>
		<link
			rel="preconnect"
			href="https://fonts.gstatic.com"
			crossorigin
		/>
		<link
			href="https://fonts.googleapis.com/css2?family=Luxurious+Script&family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap"
			rel="stylesheet"
		/>
		<link
			rel="stylesheet"
			href="https://cdn.jsdelivr.net/npm/@tabler/icons-webfont@latest/dist/tabler-icons.min.css"
		/>

		<title>With Passion</title>
		<link
			rel="stylesheet"
			href="dist/css/main.min.css"
		/>
		<head>
			<link
				rel="icon"
				href="../dist/img/favicon.png"
				type="image/png"
			/>
		</head>
	</head>

	<body>
		<!-- @include 'navSecond.kit' -->

		<section class="price wrapper lashes">
			<div class="price-title">
				<p>cennik</p>
			</div>
			<div class="price-info">
				<p>rzęsy</p>
			</div>
			<div class="prices-box">
				<div class="strip-lashes"></div>
				<div class="item lashes-one">
					<p class="left">1:1</p>
					<p>130</p>
				</div>
				<div class="item lashes-two">
					<p>1:1 uzupełnienie do 3 tygodni</p>
					<p>100</p>
				</div>
				<div class="item lashes-three">
					<p class="left">2D</p>
					<p>140</p>
				</div>
				<div class="item lashes-four">
					<p class="left">2D uzupełnienie do 3 tygodni</p>
					<p>110</p>
				</div>
				<div class="item lashes-five">
					<p class="left">3-4D</p>
					<p>160</p>
				</div>
				<div class="item lashes-six">
					<p class="left">3-4D uzupełnienie do 3 tygodni</p>
					<p>130</p>
				</div>
				<div class="item lashes-seven">
					<p class="left">efekt wispy</p>
					<p>180</p>
				</div>
				<div class="item lashes-eight">
					<p class="left">efekt wispy uzupełnienie do 3 tygodni</p>
					<p>150</p>
				</div>
				<div class="item lashes-nine">
					<p class="left">efekt mokrych rzęs</p>
					<p>170</p>
				</div>
				<div class="item lashes-ten">
					<p class="left">efekt mokrych rzęs uzupełnienie do 3 tygodni</p>
					<p>140</p>
				</div>
				<div class="item lashes-eleven">
					<p class="left">efekt kim</p>
					<p>+20</p>
				</div>

				<div class="item lashes-twelve">
					<p class="left">makijaż permanentny</p>
					<p>700</p>
				</div>

				<div class="item lashes-thirteen">
					<p class="left">henna i regulacja brwi</p>
					<p>40</p>
				</div>

				<div class="item lashes-fourteen">
					<p class="left">wosk wąsik / broda</p>
					<p>20</p>
				</div>

				<div class="item lashes-fiveteen">
					<p class="left">laminacja rzęs / brwi</p>
					<p>120</p>
				</div>
			</div>
		</section>

		<!-- @include 'footer.kit' -->

		<script src="dist/js/script.min.js"></script>
	</body>
</html>
